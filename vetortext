#include <stdio.h>

int main() {
    int valores[10]; 
    int i, maior;

    for (i = 0; i < 10; i++) {
        printf("Digite o valor %d: ", i + 1);
        scanf("%d", &valores[i]);
    }

    printf("\nValores armazenados no vetor:\n");
    for (i = 0; i < 10; i++) {
        printf("%d ", valores[i]);
    }

    maior = valores[0]; // assume o primeiro como maior
    for (i = 1; i < 10; i++) {
        if (valores[i] > maior) {
            maior = valores[i];
        }
    }

    printf("\n\nO maior valor do vetor Ã©: %d\n", maior);

    return 0;
}
